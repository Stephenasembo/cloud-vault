{
  "expo": {
    "name": "CloudVault",
    "slug": "CloudVault",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/cloudvault_icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash/light_theme.png",
      "resizeMode": "cover",
      "backgroundColor": "#F2F7FC",
      "dark": {
        "image": "./assets/splash/dark_theme.png",
        "backgroundColor": "#0A111A"
      }
    },
    "scheme": "your-app-scheme",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/cloudvault_icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.stephenasembo.cloudvault"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "53bf95ae-550f-45d4-9ac6-a41419d6bc90"
      }
    }
  }
}
